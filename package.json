    "lint:lint-staged": "lint-staged",
    "lint:eslint": "eslint --fix --cache \"**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:stylelint": "stylelint --fix --cache \"**/*.{css,scss,less,styl,vue}\"",
    "lint:prettier": "prettier --write  \"**/*.{css,scss,less,styl,js,jsx,ts,tsx,vue}\""
  },
  "dependencies": {
    "@vueuse/core": "^8.1.2",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.12",
    "qs": "^6.10.3",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@bfehub/eslint-config-vue": "^1.2.0",
    "@bfehub/stylelint-config-basic": "^1.2.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "stylelint": "^14.6.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  },
  "engines": {
    "node": "^12 || >=14"
